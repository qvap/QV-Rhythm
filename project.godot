; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Qv: Rhythm"
run/main_scene="res://Scenes/GameSpace.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="QvRhythm"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=150
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Global="*res://Scripts/Autoload/Global.gd"
Tools="*res://Scripts/Autoload/Tools.gd"
Conductor="*res://Scenes/Autoload/Conductor.tscn"
Settings="*res://Scripts/Autoload/Settings.gd"
Scoring="*res://Scripts/Autoload/Scoring.gd"
LoadScene="*res://Scenes/Autoload/SceneLoader.tscn"

[debug]

settings/crash_handler/message="Если вы столкнулись с ошибкой, сообщите об этом разработчику."
settings/stdout/print_fps=true
shader_language/warnings/treat_warnings_as_errors=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[editor]

naming/default_signal_callback_name="{signal_name}"
naming/default_signal_callback_to_self_name="{signal_name}"
naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/SmoothScroll/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "green",
"res://CustomMaps/": "orange",
"res://Scenes/": "purple",
"res://Scripts/": "yellow",
"res://addons/": "gray"
}

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

